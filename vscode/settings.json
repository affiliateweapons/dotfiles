  "better-comments.multilineComments": true,
  "better-comments.tags": [
    // regular comments
    {
      "tag": "#",
      "color": "#bb9af7",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": true
    },
    // disabled
    {      
      "tag": "@disabled",
      "color": "#343b58",
      "strikethrough": false,
      "underline": true,
      "backgroundColor": "#transparent",
      "bold": false,
      "italic": true
    },
    // important notes
    {
      "tag": "!",
      "color": "#ff0000",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    // bindkey input key
    {
      "tag": ">",
      "color": "#f7768e",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": true,
      "italic": false
    },
    // bindkey function
    {
      "tag": "<",
      "color": "#9ece6a",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#transparent",
      "bold": false,
      "italic": true
    },
    // unfinished/unsolved issue
    {      
      "tag": "@?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": true
    },
    // shorthand for function
    {      
      "tag": "@f",
      "color": "white",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#34548a",
      "bold": false,
      "italic": true
    },
    // plugin
    {      
      "tag": "@plugin",
      "color": "white",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#3498DB",
      "bold": false,
      "italic": true
    },
    // requires
    {      
      "tag": "@require",
      "color": "#545c7e",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#3498DB",
      "bold": false,
      "italic": true
    },
    {
      "tag": "@plugin",
      "color": "white",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#3498DB",
      "bold": false,
      "italic": true
    },
    // refactor
    {
      "tag": "@refactor",
      "color": "#B267E6",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": true,
      "italic": false
    },

    // deprecated function
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    // unfinished function/tasks
    {
      "tag": "todo",
      "color": "#ff9e64",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": true,
      "italic": false
    },
    {
      "tag": "idea",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": true,
      "italic": false
    },

    {
      "tag": "@",
      "color": "#5a4a78",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": true
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#545c7e",
      "bold": false,
      "italic": false
    },
  ],
